{
  "dataset": "daily_orders",
  "schema": {
    "order_id": "int",
    "order_date": "str",
    "customer_id": "int",
    "channel": "str",
    "country": "str",
    "items": "int",
    "order_value": "float",
    "discount": "float",
    "is_return": "int"
  },
  "rules": [
    {
      "name": "order_id_unique",
      "type": "unique",
      "column": "order_id"
    },
    {
      "name": "non_null_core",
      "type": "non_null",
      "columns": [
        "order_id",
        "order_date",
        "customer_id",
        "channel",
        "order_value"
      ]
    },
    {
      "name": "items_range",
      "type": "range",
      "column": "items",
      "min": 1,
      "max": 25
    },
    {
      "name": "order_value_positive",
      "type": "range",
      "column": "order_value",
      "min": 0.5,
      "max": 5000.0
    },
    {
      "name": "discount_range",
      "type": "range",
      "column": "discount",
      "min": 0.0,
      "max": 0.9
    },
    {
      "name": "return_rate_reasonable",
      "type": "rate_max",
      "column": "is_return",
      "max": 0.25
    }
  ],
  "drift": {
    "numeric_columns": [
      "items",
      "order_value",
      "discount"
    ],
    "categorical_columns": [
      "channel",
      "country"
    ],
    "alpha": 0.01
  }
}